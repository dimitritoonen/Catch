<?xml version="1.0" encoding="utf-8" ?>
<system.diagnostics xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">

  <sources>

    <source name="Account" xdt:Locator="Match(name)">
      <listeners>
        <add name="AzureDiagnostics" xdt:Transform="Remove" xdt:Locator="Condition(@name='AzureDiagnostics')" />
      </listeners>
    </source>

    <source name="Global" xdt:Locator="Match(name)">
      <listeners>
        <add name="AzureDiagnostics" xdt:Transform="Remove" xdt:Locator="Condition(@name='AzureDiagnostics')" />
      </listeners>
    </source>
  </sources>

  <trace>
    <listeners>
      <add name="AzureDiagnostics" xdt:Transform="Remove" xdt:Locator="Condition(@name='AzureDiagnostics')" />
    </listeners>
  </trace>

  <sharedListeners>
    <add name="AzureDiagnostics" xdt:Transform="Remove" xdt:Locator="Condition(@name='AzureDiagnostics')" />
  </sharedListeners>
</system.diagnostics>